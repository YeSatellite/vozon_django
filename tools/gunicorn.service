[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=car
Group=www-data
WorkingDirectory=/home/car/car
environment=PYTHONIOENCODING="UTF-8", LANG="en_US.utf8", LC_ALL="en_US.UTF-8", LC_LANG="en_US.UTF-8"
ExecStart=/home/car/.virenv/car/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/car/car/car.sock config.wsgi:application


[Install]
WantedBy=multi-user.target