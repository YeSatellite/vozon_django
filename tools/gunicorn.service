[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=car
Group=www-data
WorkingDirectory=/home/car/car
ExecStart=/home/car/.virenv/car/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/car/car/car.sock config.wsgi:application -e LANG='en_US.UTF-8' -e LC_ALL='en_US.UTF-8'


[Install]
WantedBy=multi-user.target